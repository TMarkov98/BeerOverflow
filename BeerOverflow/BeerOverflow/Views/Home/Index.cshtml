@{
    ViewData["Title"] = "Home Page";
}


<h1 align="center">
    Welcome to Beer Overflow!
</h1>
<div align="center" id="carouselExampleControls" class="carousel slide" data-ride="carousel">

    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="d-block w-100" src="~/images/home/Beer-Image-HD.png" alt="First slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images//home/Beer-Image.png" alt="Second slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/home/Beer-Wallpaper-HD.png" alt="Third slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/home/Free-DownloadBeer-Background.png" alt="Third slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/home/HD-Beer-Images.png" alt="Third slide">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>